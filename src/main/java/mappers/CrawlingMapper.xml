<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="CrawlingDao">
	<select id="selectList" resultType="CrawVO">
		select * from item
	</select>
	<select id="selectStore" resultType="CrawVO">
		select * from store_list
	</select>
	
	<select id="CrawlingTopList" resultType="CrawVO">
		select * from item where category_num=2
	</select>
</mapper>